// Prompt the user to enter the student's marks
let marks = prompt("Enter student marks between 0 and 100");

// Convert the marks to a number
marks = Number(marks);

// Check if the marks is between 0 and 100
if (marks >= 0 && marks <= 100) {
  // Determine the grade based on the marks
  let grade;
  if (marks > 79) {
    grade = "A";
  } else if (marks >= 60) {
    grade = "B";
  } else if (marks >= 50) {
    grade = "C";
  } else if (marks >= 40) {
    grade = "D";
  } else {
    grade = "E";
  }

  // log the grade to the console
  console.log("The student's grade is: " + grade);
} else {
  // log an error message if the marks is not valid
  console.log("Invalid marks entered!");
}
